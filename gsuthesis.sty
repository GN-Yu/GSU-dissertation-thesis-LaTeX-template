      \@mkboth{\MakeUppercase\listtablename}
              {\MakeUppercase\listtablename}}
    \addcontentsline{toc}{chapter}{\numberline{}\listtablename}
    \@starttoc{lot}
    \if@restonecol\twocolumn\fi
    }
\newlength{\captionwidth}
\setlength{\captionwidth}{400pt}
\long\def\@custommakecaption#1#2{%
  \vskip\abovecaptionskip
  \sbox\@tempboxa{#1: #2}%
  \ifdim \wd\@tempboxa >\captionwidth
    \begin{center}\parbox[t]{\captionwidth}{#1: #2}\end{center}
  \else
    \global \@minipagefalse
    \hb@xt@\captionwidth{\hfil\box\@tempboxa\hfil}%
  \fi
  \vskip\belowcaptionskip}
\renewcommand{\caption}{
    \refstepcounter\@captype \@dblarg{\@customcaption\@captype}
}
\long\def\@customcaption#1[#2]#3{\par\addcontentsline{\csname
  ext@#1\endcsname}{#1}{\protect\numberline{\csname
  the#1\endcsname}{\ignorespaces #2}}\begingroup
    \@parboxrestore
    \small
    \@custommakecaption{\csname fnum@#1\endcsname}{\ignorespaces #3}\par
  \endgroup}
